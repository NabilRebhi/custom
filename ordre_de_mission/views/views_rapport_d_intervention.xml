                                   <control>
                                       <create name ="add_line_control" string ="Add a record" />
                                   </control>
                                   <field name="description" />
                                   <field name="status" />
                                   <field name="employes" />
                               </tree>
                           </field>
                       </page>
                </notebook>
           </sheet>
       </form>
   </field>
